{
  "author": "Shaun Hamilton",
  "dependencies": {
    "logover": "2.0.0",
    "promisify-child-process": "4.1.1"
  },
  "devDependencies": {
    "@babel/parser": "7.19.6",
    "@babel/types": "7.19.4",
    "@tsconfig/node18-strictest-esm": "1.0.1",
    "@types/node": "^18.11.5",
    "babeliser": "0.4.1",
    "tsup": "6.3.0",
    "typescript": "4.8.4"
  },
  "main": "./dist/index.js",
  "name": "node-bug",
  "scripts": {
    "build": "tsup",
    "test": "LOG_LEVEL=debug tsup test/index.ts --format esm --outDir distest  && node distest/index.js",
    "posttest": "rm -rf distest"
  },
  "files": [
    "build"
  ],
  "tsup": {
    "clean": true,
    "entry": [
      "src/index.ts"
    ],
    "sourcemap": true,
    "splitting": false,
    "target": "node18"
  },
  "type": "module",
  "version": "0.1.0"
}
